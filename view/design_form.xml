<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="product_exists"/>
    <field name="product_exists"/>
    <label name="manual_name"/>
    <field name="manual_name"/>
    <label name="manual_code"/>
    <field name="manual_code"/>
    <label name="product"/>
    <field name="product"/>
    <label name="design_date"/>
    <field name="design_date"/>
    <label name="company"/>
    <field name="company"/>
    <label name="currency"/>
    <field name="currency"/>
    <label name="quotation_uom"/>
    <field name="quotation_uom"/>
    <label name="sale_uom"/>
    <field name="sale_uom"/>
    <group id="templates" colspan="4">
      <label name="template"/>
      <field name="template"/>
      <button name="update"/>
    </group>
    <newline/>
    <notebook colspan="6">
        <page name="attributes">
          <label name="process_by"/>
          <field name="process_by"/>
          <label name="process_date"/>
          <field name="process_date"/>
         <field name="attributes" colspan="4" height="800"/>
        </page>
        <page name="prices">
            <label name="quoted_by"/>
            <field name="quoted_by"/>
            <label name="quotation_date"/>
            <field name="quotation_date"/>
            <field name="suppliers" colspan="4"/>
            <field name="prices" colspan="4"/>
        </page>
        <page name="product_codes">
            <field name="product_codes" colspan="4"/>
        </page>
        <page name="objects">
            <field name="objects" colspan="4"/>
        </page>
    </notebook>
    <label name="state"/>
    <field name="state"/>
    <button name="process" />
    <button name="cancel"/>
    <button name="create_prices"/>
</form>
